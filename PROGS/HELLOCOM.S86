; Simple database used by HUSIS Applications

.text $100
jmp start

msg:
    db "Hello World!",0

start:
    mov ax, $4000
    mov bx, 0
    mov dx, @msg
    mov cx, 12
    int $21
infinite:
    hlt
    jmp infinite

end:

